{
  "functions": { 
      "source": "application/.output/server",
        "runtime": "nodejs16"
  },
  "hosting": {
    "public": "application/.output/public",
    "cleanUrls": true,
    "rewrites": [{ "source": "**", "function": "server" }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
